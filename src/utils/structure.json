        {
          "label": "Full Name",
          "placeholder": "Enter your full name",
          "fieldType": "text",
          "errorColor": "#ff4d4f",
          "focusErrorBgColor": "#DCFFDB",
          "focusErrorBorderColor": "#008000",
          "placeHoldercolor": "#000",
          "readOnly": true,
          "validation": {
            "required": "N",
            "pattern": "^[A-Za-z]{2,50}$",
            "errorMessage": "Name must be 2-50 characters and contain only letters and spaces."
          }
        },
        {
          "key": "dob",
          "label": "Date of Birth",
          "placeholder": "YYYY-MM-DD",
          "fieldType": "text",
          "subType": "date",
          "validation": {
            "required": "N",
            "errorMessage": "Select date of birth"
          }
        },
        {
          "key": "citizenshipnumber",
          "label": "Citizenship Number",
          "placeholder": "Enter Citizenship Number",
          "fieldType": "text",
          "textSecurity": "$",
          "validation": {
            "required": "N",
            "errorMessage": "Name must be 2-50 characters and contain only letters and spaces."
          }
        },
        {
          "key": "mobilenumber",
          "label": "Mobile Number",
          "placeholder": "Enter Mobile Number",
          "fieldType": "text",
          "subType": "number",
          "validation": {
            "required": "N",
            "pattern": "^[6-9]\\d{9}$",
            "errorMessage": "Enter a valid 10-digit mobile number starting with 6-9."
